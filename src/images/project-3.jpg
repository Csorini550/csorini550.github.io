<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400">
  <rect width="600" height="400" fill="#f0f0f0" />
  
  <!-- Background pattern -->
  <pattern id="grid" width="20" height="20" patternUnits="userSpaceOnUse">
    <path d="M 20 0 L 0 0 0 20" fill="none" stroke="#e0e0e0" stroke-width="1" />
  </pattern>
  <rect width="600" height="400" fill="url(#grid)" />
  
  <!-- Color gradient -->
  <defs>
    <linearGradient id="gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ff006e;stop-opacity:0.7" />
      <stop offset="100%" style="stop-color:#8338ec;stop-opacity:0.7" />
    </linearGradient>
  </defs>
  
  <!-- Main content - Simulated camera view with object detection -->
  <rect x="100" y="80" width="400" height="240" rx="5" fill="#333" />
  
  <!-- Scene elements -->
  <g transform="translate(100, 80)">
    <!-- Background -->
    <rect x="0" y="0" width="400" height="240" fill="#222" />
    
    <!-- Sun/moon -->
    <circle cx="350" cy="40" r="20" fill="#ffbe0b" opacity="0.8" />
    
    <!-- Mountains -->
    <polygon points="0,240 0,180 50,150 100,180 150,130 200,160 250,140 300,170 350,150 400,180 400,240" fill="#444" />
    
    <!-- Person outline with bounding box -->
    <g transform="translate(130, 140)">
      <!-- Bounding box -->
      <rect x="-30" y="-70" width="60" height="110" fill="none" stroke="#ff006e" stroke-width="2" stroke-dasharray="5,3" />
      
      <!-- Person silhouette -->
      <circle cx="0" cy="-55" r="15" fill="#777" /> <!-- Head -->
      <rect x="-10" y="-40" width="20" height="30" fill="#777" /> <!-- Body -->
      <line x1="-10" y1="-30" x2="-25" y2="-10" stroke="#777" stroke-width="6" /> <!-- Left arm -->
      <line x1="10" y1="-30" x2="25" y2="-10" stroke="#777" stroke-width="6" /> <!-- Right arm -->
      <line x1="-5" y1="-10" x2="-10" y2="40" stroke="#777" stroke-width="8" /> <!-- Left leg -->
      <line x1="5" y1="-10" x2="10" y2="40" stroke="#777" stroke-width="8" /> <!-- Right leg -->
      
      <!-- Label -->
      <rect x="-30" y="-90" width="60" height="20" rx="3" fill="#ff006e" />
      <text x="0" y="-75" font-family="Arial" font-size="12" text-anchor="middle" fill="white">Person</text>
      <text x="0" y="-90" font-family="Arial" font-size="10" text-anchor="middle" fill="white" opacity="0.8">98.7%</text>
    </g>
    
    <!-- Car outline with bounding box -->
    <g transform="translate(270, 190)">
      <!-- Bounding box -->
      <rect x="-50" y="-30" width="100" height="50" fill="none" stroke="#3a86ff" stroke-width="2" stroke-dasharray="5,3" />
      
      <!-- Car silhouette -->
      <rect x="-40" y="-10" width="80" height="25" rx="5" fill="#888" /> <!-- Body -->
      <rect x="-30" y="-25" width="60" height="15" rx="3" fill="#888" /> <!-- Top -->
      <circle cx="-25" cy="15" r="10" fill="#555" /> <!-- Left wheel -->
      <circle cx="25" cy="15" r="10" fill="#555" /> <!-- Right wheel -->
      
      <!-- Label -->
      <rect x="-30" y="-50" width="60" height="20" rx="3" fill="#3a86ff" />
      <text x="0" y="-35" font-family="Arial" font-size="12" text-anchor="middle" fill="white">Car</text>
      <text x="0" y="-50" font-family="Arial" font-size="10" text-anchor="middle" fill="white" opacity="0.8">95.2%</text>
    </g>
    
    <!-- Tree with bounding box -->
    <g transform="translate(50, 170)">
      <!-- Bounding box -->
      <rect x="-20" y="-70" width="40" height="70" fill="none" stroke="#8338ec" stroke-width="2" stroke-dasharray="5,3" />
      
      <!-- Tree silhouette -->
      <rect x="-5" y="-20" width="10" height="20" fill="#555" /> <!-- Trunk -->
      <polygon points="0,-70 -20,-20 20,-20" fill="#555" /> <!-- Top -->
      
      <!-- Label -->
      <rect x="-30" y="-90" width="60" height="20" rx="3" fill="#8338ec" />
      <text x="0" y="-75" font-family="Arial" font-size="12" text-anchor="middle" fill="white">Tree</text>
      <text x="0" y="-90" font-family="Arial" font-size="10" text-anchor="middle" fill="white" opacity="0.8">89.5%</text>
    </g>
  </g>
  
  <!-- Camera frame and controls -->
  <rect x="100" y="80" width="400" height="240" rx="5" fill="none" stroke="#333" stroke-width="5" />
  <circle cx="130" cy="100" r="8" fill="#ff006e" /> <!-- Record button -->
  
  <!-- Overlay stats -->
  <g transform="translate(450, 100)">
    <rect x="-70" y="-15" width="100" height="20" rx="3" fill="rgba(0,0,0,0.6)" />
    <text x="-30" y="0" font-family="monospace" font-size="12" fill="white">FPS: 60</text>
  </g>
  
  <!-- Title -->
  <rect x="100" y="340" width="400" height="35" rx="5" fill="rgba(255,255,255,0.8)" />
  <text x="300" y="365" font-family="Arial" font-size="20" text-anchor="middle" fill="#333">Computer Vision Object Detection</text>
</svg> 