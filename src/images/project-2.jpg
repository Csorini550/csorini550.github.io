<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400">
  <rect width="600" height="400" fill="#f0f0f0" />
  
  <!-- Background pattern -->
  <pattern id="grid" width="20" height="20" patternUnits="userSpaceOnUse">
    <path d="M 20 0 L 0 0 0 20" fill="none" stroke="#e0e0e0" stroke-width="1" />
  </pattern>
  <rect width="600" height="400" fill="url(#grid)" />
  
  <!-- Color gradient -->
  <defs>
    <linearGradient id="gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#8338ec;stop-opacity:0.7" />
      <stop offset="100%" style="stop-color:#3a86ff;stop-opacity:0.7" />
    </linearGradient>
  </defs>
  
  <!-- Main content -->
  <g transform="translate(50, 50)">
    <!-- Text analysis visualization -->
    <rect x="0" y="0" width="500" height="60" rx="5" fill="rgba(255,255,255,0.9)" stroke="#ccc" />
    <text x="20" y="35" font-family="monospace" font-size="18" fill="#333">The quick brown fox jumps over the lazy dog</text>
    
    <!-- NLP analysis -->
    <g transform="translate(0, 80)">
      <!-- Part of speech tags -->
      <rect x="0" y="0" width="40" height="30" rx="3" fill="#3a86ff" opacity="0.7" />
      <text x="20" y="20" font-family="monospace" font-size="12" text-anchor="middle" fill="white">DET</text>
      
      <rect x="45" y="0" width="60" height="30" rx="3" fill="#ff006e" opacity="0.7" />
      <text x="75" y="20" font-family="monospace" font-size="12" text-anchor="middle" fill="white">ADJ</text>
      
      <rect x="110" y="0" width="60" height="30" rx="3" fill="#ff006e" opacity="0.7" />
      <text x="140" y="20" font-family="monospace" font-size="12" text-anchor="middle" fill="white">ADJ</text>
      
      <rect x="175" y="0" width="45" height="30" rx="3" fill="#8338ec" opacity="0.7" />
      <text x="197.5" y="20" font-family="monospace" font-size="12" text-anchor="middle" fill="white">NOUN</text>
      
      <rect x="225" y="0" width="65" height="30" rx="3" fill="#3a86ff" opacity="0.7" />
      <text x="257.5" y="20" font-family="monospace" font-size="12" text-anchor="middle" fill="white">VERB</text>
      
      <rect x="295" y="0" width="50" height="30" rx="3" fill="#ff006e" opacity="0.7" />
      <text x="320" y="20" font-family="monospace" font-size="12" text-anchor="middle" fill="white">PREP</text>
      
      <rect x="350" y="0" width="40" height="30" rx="3" fill="#3a86ff" opacity="0.7" />
      <text x="370" y="20" font-family="monospace" font-size="12" text-anchor="middle" fill="white">DET</text>
      
      <rect x="395" y="0" width="50" height="30" rx="3" fill="#ff006e" opacity="0.7" />
      <text x="420" y="20" font-family="monospace" font-size="12" text-anchor="middle" fill="white">ADJ</text>
      
      <rect x="450" y="0" width="50" height="30" rx="3" fill="#8338ec" opacity="0.7" />
      <text x="475" y="20" font-family="monospace" font-size="12" text-anchor="middle" fill="white">NOUN</text>
    </g>
    
    <!-- Named entity recognition -->
    <g transform="translate(0, 130)">
      <rect x="0" y="0" width="500" height="40" rx="5" fill="rgba(255,255,255,0.7)" stroke="#ccc" />
      <text x="20" y="25" font-family="monospace" font-size="16" fill="#333">Named Entity Recognition:</text>
      
      <rect x="50" y="50" width="130" height="30" rx="3" fill="#ff006e" opacity="0.7" />
      <text x="115" y="70" font-family="monospace" font-size="14" text-anchor="middle" fill="white">ANIMAL: fox</text>
      
      <rect x="200" y="50" width="130" height="30" rx="3" fill="#8338ec" opacity="0.7" />
      <text x="265" y="70" font-family="monospace" font-size="14" text-anchor="middle" fill="white">ANIMAL: dog</text>
    </g>
    
    <!-- Sentiment analysis -->
    <g transform="translate(150, 220)">
      <rect x="0" y="0" width="200" height="40" rx="5" fill="rgba(255,255,255,0.7)" stroke="#ccc" />
      <text x="100" y="25" font-family="monospace" font-size="16" text-anchor="middle" fill="#333">Sentiment: Neutral</text>
      
      <!-- Sentiment gauge -->
      <rect x="-50" y="50" width="300" height="10" rx="5" fill="#e0e0e0" />
      <rect x="-50" y="50" width="150" height="10" rx="5" fill="#3a86ff" />
      <circle cx="100" cy="55" r="8" fill="#ff006e" />
    </g>
  </g>
  
  <!-- Title -->
  <rect x="100" y="20" width="400" height="50" rx="10" fill="rgba(255,255,255,0.8)" />
  <text x="300" y="55" font-family="Arial" font-size="22" text-anchor="middle" fill="#333">Natural Language Processing API</text>
</svg> 